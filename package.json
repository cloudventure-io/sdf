  },
  "homepage": "https://github.com/cloudventure-io/sdf#readme",
  "pnpm": {
    "overrides": {
      "jsonpath-plus": "^10.0.0"
    },
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ],
    "onlyBuiltDependencies": [
      "@cdktf/node-pty-prebuilt-multiarch",
      "esbuild"
    ]
