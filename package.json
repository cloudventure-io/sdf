    "@cdktf/provider-aws": "^18.0.0",
    "@cdktf/provider-null": "^9.0.0",
    "@types/aws-lambda": "^8.10.123",
    "ajv": "^8.12.0",
    "cdktf": "^0.19.0",
    "cdktf-cli": "^0.19.0",
    "change-case": "^4",
    "commander": "^11.0.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.19.4",
    "json-schema-to-typescript": "^13.1.1",
    "mustache": "^4.2.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@cloudventure/jest-transformer-esbuild": "^1.0.13",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.5",
    "@types/mustache": "^4.2.3",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "esbuild --platform=node --target=node18 --outfile=tmp/prepare.js --bundle --sourcemap=inline --external:esbuild --external:typescript --log-level=error prepare.ts",
    "postprepare": "node tmp/prepare.js",
    "prebuild": "yarn prepare",
    "build": "node tmp/build.js",
    "postbuild": "yarn run tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --coverage",
    "typecheck": "yarn run tsc --noEmit --emitDeclarationOnly false",
    "prelint": "yarn run -s typecheck",
    "lint": "yarn run eslint ./"
  }
}
